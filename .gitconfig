	email = t_abraham@apple.com
[core]
  excludesfile = ~/.gitignore.global
  attributesfile = ~/.gitattributes
[push]
  default = tracking
[diff "sqlite"]
  textconv  = ~/sqlite2txt
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
